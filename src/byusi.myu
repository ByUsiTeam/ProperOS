fn encrypt(filePath)

call(Result,"mlua","FileCtrl.file_to_hex",filePath)
otob("utf-8",Result,b)
sr(b,"0","�",b)
sr(b,"1","你",b)
sr(b,"2","好",b)
sr(b,"3","，",b)
sr(b,"4","世",b)
sr(b,"5","界",b)
sr(b,"6","！",b)
sr(b,"7","锟斤拷",b)
sr(b,"8","加",b)
sr(b,"9","油",b)
