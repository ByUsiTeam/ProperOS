public boolean stopphp(){
  // kill -9 `lsof -i :22 -t` 查出使用22端口的进程pid，并杀掉该进程
  boolean b=false;
  fn("shell");
  String[] cmd = new String[] {"kill","-9","lsof -i :21200"};
  exec(cmd);
  b=true;
  return b;
}