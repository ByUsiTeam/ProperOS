public void del(String dir){
  boolean is=i.fe(dir);
  if(is){
    boolean isdel=i.fd(dir);
  }
  for(Object va:i.fl(dir)){
    if(!i.fd(dir+"/"+va)){
      del(dir+"/"+va);
    }
  }
  i.fd(dir);
}
